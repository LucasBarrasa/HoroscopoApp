<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graph_main"
    app:startDestination="@id/horoscopoFragment">

    <fragment
        android:id="@+id/horoscopoFragment"
        android:name="com.finappproyect.apphoroscopo.ui.horoscopo.HoroscopoFragment"
        android:label="fragment_horoscopo"
        tools:layout="@layout/fragment_horoscopo" >
        <action
            android:id="@+id/action_horoscopoFragment_to_horoscopoDetail"
            app:destination="@id/horoscopoDetail" />
    </fragment>

    <fragment
        android:id="@+id/luckFragment"
        android:name="com.finappproyect.apphoroscopo.ui.luck.LuckFragment"
        android:label="fragment_luck"
        tools:layout="@layout/fragment_luck" />

    <fragment
        android:id="@+id/palmistryFragment"
        android:name="com.finappproyect.apphoroscopo.ui.palmistry.PalmistryFragment"
        android:label="fragment_palmistry"
        tools:layout="@layout/fragment_palmistry" />
    <activity
        android:id="@+id/horoscopoDetail"
        android:name="com.finappproyect.apphoroscopo.ui.horoscopo.horoscopoDetail.HoroscopoDetail"
        android:label="activity_horoscopo_detail"
        tools:layout="@layout/activity_horoscopo_detail" />


</navigation>